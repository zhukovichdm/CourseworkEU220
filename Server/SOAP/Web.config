    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <connectionStrings>
    <add name="Coursework" connectionString="data source=DESKTOP-ST0N5M1\SQLEXPRESS;Initial Catalog=Coursework;Integrated Security=True;" providerName="System.Data.SqlClient" />
    <add name="CourseworkDB" connectionString="data source=DESKTOP-ST0N5M1\SQLEXPRESS;Initial Catalog=CourseworkDB;Integrated Security=True;" providerName="System.Data.SqlClient" />
    <add name="CourseworkDBContext" connectionString="metadata=res://*/SoapModel.csdl|res://*/SoapModel.ssdl|res://*/SoapModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=DESKTOP-ST0N5M1\SQLEXPRESS;initial catalog=CourseworkDB;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
